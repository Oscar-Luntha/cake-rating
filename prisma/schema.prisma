generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cake {
  id          String   @id @default(cuid())
  name        String
  price       Float
  imageUrl    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}


model WallOfFame {
  id     Int      @id @default(1)
  images String[] // Postgres supports native array types for String[]
}
